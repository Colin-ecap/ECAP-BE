{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "framework": "node",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 